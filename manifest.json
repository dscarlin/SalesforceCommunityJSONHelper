{
  "name": "OmniScript JSON Data Viewer",
  "version": "0.1.0",
  "description": "OmniScript JSON Data Viewer Window",
  "manifest_version": 3,
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["loadContentScripts.js"]
}],
"browser_action": {
    "default_icon": "jsonIcon.png",
    "icons": {
     
      "128": "jsonIcon.png"
    }
},
    "icons": {
        "128": "jsonIcon.png"
      },
   
      "web_accessible_resources": [
        {
          "resources": [
            "SalesforceNetworkResponse.js",
            "WindowLauncherButton.js", 
            "DraggableElement.js", 
            "JsonWindow.js", 
            "WindowShowHideHandler.js",
            "index.js" 
        ],
          "matches": [ "*://*/*" ]
        }
      ]
}
