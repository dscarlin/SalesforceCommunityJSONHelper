{
  "name": "OmniStudio Sites JSON Helper",
  "version": "0.1.1",
  "description": "View OmniScript Data JSON running in Salesforce Community and parse network payloads easily",
  "manifest_version": 3,
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["loadContentScripts.js"]
}],
"browser_action": {
    "default_icon": "jsonIcon.png",
    "icons": {
     
      "128": "jsonIcon.png"
    }
},
    "icons": {
        "128": "jsonIcon.png"
      },
   
      "web_accessible_resources": [
        {
          "resources": [
            "DisplayField.js",
            "ClickableBlock.js",
            "Repeat.js",
            "SalesforceNetworkPayload.js",
            "WindowLauncherButton.js", 
            "DraggableMixin.js", 
            "JsonWindow.js", 
            "WindowShowHideHandler.js",
            "index.js" 
        ],
          "matches": [ "*://*/*" ]
        }
      ]
}
